algoritmo "semnome"
// Função :
// Autor :
// Data : 01/06/2021
// Seção de Declarações 
var
   ano, nasc, idade: inteiro
inicio
     escreva("em que ano estamos? ")
     leia(ano)
     escreva("em que ano voce nasceu? ")
     leia(nasc)
     idade <- ano - nasc
     escreval ("em", ano, " voce tera ", idade, " anos.")
     se (idade >= 21) entao
     escreval("e voce ja tera atingido a maioridade. ")
fimse

fimalgoritmo                                                      .
