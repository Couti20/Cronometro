algoritmo "semnome"

var
    ano, nasc,  idade:inteiro
inicio
      Escreva("Em que ano estamos ? ")
      leia(ano)
      EScreva ("Em que ano você nasceu? ")
      leia(nasc)
      idade <- ano - nasc
      Escreval("em ",  ano, "voce tera ", idade, " anos ")
      se(idade>= 21) entao
      Escreval ("E voce ja tera atingindo a maioridade. ")




fimalgoritmo

