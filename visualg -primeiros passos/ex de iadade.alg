algoritmo "Creuza01"


var
       ano_atual: inteiro
       ano_nasci: inteiro
       idade: inteiro
inicio
      Escreva("Em que ano nós estamos ? ")
      Leia(ano_atual)
      Escreva("Em que ano eu nasci ?")
      leia(ano_nasci)
      idade <- ano_atual - ano_nasci
      Escreva("Minha idade sera ", idade)
fimalgoritmo
